{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "rm-adf"
		},
		"AzureBlobStorage1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorage1'"
		},
		"dsazuresql_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'dsazuresql'"
		},
		"dsonpremsqlserver_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'dsonpremsqlserver'"
		},
		"dsazuresql_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "shannuserver.database.windows.net"
		},
		"dsazuresql_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "shannudb"
		},
		"dsazuresql_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "shannu"
		},
		"dsonpremsqlserver_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "DESKTOP-5N5I1VK\\SQLEXPRESS"
		},
		"dsonpremsqlserver_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "Test_db"
		},
		"dsonpremsqlserver_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "rajesh"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "rm-adf_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureBlobStorage1_connectionString": {
						"value": "[parameters('AzureBlobStorage1_connectionString')]"
					},
					"dsazuresql_password": {
						"value": "[parameters('dsazuresql_password')]"
					},
					"dsonpremsqlserver_password": {
						"value": "[parameters('dsonpremsqlserver_password')]"
					},
					"dsazuresql_properties_typeProperties_server": {
						"value": "[parameters('dsazuresql_properties_typeProperties_server')]"
					},
					"dsazuresql_properties_typeProperties_database": {
						"value": "[parameters('dsazuresql_properties_typeProperties_database')]"
					},
					"dsazuresql_properties_typeProperties_userName": {
						"value": "[parameters('dsazuresql_properties_typeProperties_userName')]"
					},
					"dsonpremsqlserver_properties_typeProperties_server": {
						"value": "[parameters('dsonpremsqlserver_properties_typeProperties_server')]"
					},
					"dsonpremsqlserver_properties_typeProperties_database": {
						"value": "[parameters('dsonpremsqlserver_properties_typeProperties_database')]"
					},
					"dsonpremsqlserver_properties_typeProperties_userName": {
						"value": "[parameters('dsonpremsqlserver_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}